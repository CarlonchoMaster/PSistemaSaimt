                End If
                CExpSG_dgvResultado.DataSource = CExpSG_oTable
                CExpSG_gvResultado.BestFitColumns()

            Catch ex As Exception
                SaimtMessageBox.mostrarAlertaInformativa(ex.Message)
            End Try


        End If
    End Sub

